@charset "UTF-8";
@import url('member.css');

/*breadcrumb*/
#breadcrumb {width: 100%; background: #f1f3f8; padding: 11rem 0;}
#breadcrumb > .wrap {}
#breadcrumb ul {display: flex;flex-wrap: wrap;align-items: center; }
#breadcrumb ul li {font-size: 16rem; padding-right: 45rem; position: relative;}
#breadcrumb ul li::after {content: ''; display: block; width: 5px; height: 9px; background: url('../img/sub/arrow_bc.gif') no-repeat center/100%; position: absolute; top: 50%; right: calc((45rem - 5px)/2); transform: translateY(-50%);}
#breadcrumb ul li:last-child {font-weight: 600;padding-right: 0;}
#breadcrumb ul li:last-child::after {display: none;}
#breadcrumb ul li a {line-height: 1;}
#breadcrumb ul li.home a {display: block; text-indent: -9999rem; background: url('../img/sub/icon_home.png') no-repeat center/100%; width: 17rem; height: 16rem;}

#inner {margin-top: 50rem;margin-bottom: 80rem;}
#inner > .wrap {display: flex;flex-wrap: wrap; align-items: flex-start;}

/*snb*/
#snb {width: 280rem;background: #284199;border-radius: 15rem;margin-right: 70rem;}
#snb h2 {font-size: 27rem; font-weight: 700;color: #fff; padding: 43rem 20rem;text-align: center; line-height: 1;}
#snb .snb {background: #fff;border-radius: 15rem;border: 1px solid #e1e1e1;padding: 13rem 20rem 30rem;}
#snb .snb .depth01 > li > a {display: block; font-size: 17rem; font-weight: 500; color: #666666;padding: 17rem 40rem 17rem 0;border-bottom: 1px solid #e1e1e1;position: relative;}
#snb .snb .depth01 > li.has > a.on, #snb .snb .depth01 > li:hover > a {font-weight: 700;font-size: 18rem; color: #284199; border-bottom: 2px solid #284199;padding: 17rem 40rem 15rem 0;}
#snb .snb .depth01 > li > a::after {content: ''; display: block; position: absolute; width: 28rem; height: 28rem; background: url('../img/sub/arrow_snb.png') no-repeat left center/200%; top: 50%; right: 0; transform: translateY(-50%);}
#snb .snb .depth01 > li.has > a.on::after, #snb .snb .depth01 > li:hover > a::after {background-position: right center;}
#snb .snb li > a[target="_blank"] > span::after {content: ''; display: inline-block; width: 12rem; height: 12rem; margin-left:4rem; background: url('../img/sub/blank.png') no-repeat 0 0/200%;}
#snb .snb li > a[target="_blank"]:hover > span::after {background-position: 100% 0;}
#snb .snb .depth02 {display: none;}
#snb .snb .depth02 > li > a {display: block; font-size: 16rem; color: #323232;padding: 17rem 12rem; border-bottom: 1px solid #e1e1e1;}
#snb .snb .depth02 > li > a > span {display: block; padding-left: 12rem; position: relative; line-height: 1.3;}
#snb .snb .depth02 > li > a > span::before {content: ''; display: block; position: absolute; width: 2px; height: 2px; background: #565656; top: 8rem; left: 0;}
#snb .snb .depth02 > li:last-child > a {border-bottom: 2px solid #284199;}
#snb .snb .depth02 > li > a.on, #snb .snb .depth02 > li:hover > a {font-weight: 600; color: #284199; letter-spacing: -0.05em;}
@media all and (max-width:1290px) {
    #snb {display: none;}
}

/*content*/
#content {width: calc(100% - 350rem);}
#content h3 {font-size: 30rem;font-weight: 600; margin-bottom: 30rem; margin-top: -9rem;color: #000000;}
@media all and (max-width:1290px) {
    #content {width: 100%;}
}

